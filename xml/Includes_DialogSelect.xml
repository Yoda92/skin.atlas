			<!-- Background second panel /-->
			<control type="image">
				<top>123</top>
				<left>34</left>
				<texture border="5" colordiffuse="secondary700">shapes/white_rounded.png</texture>
				<height>220</height>
				<width>1329</width>
			</control>
			<control type="panel" id="11">
				<top>123</top>
				<left>34</left>
				<orientation>horizontal</orientation>
				<height>220</height>
				<width>1329</width>
				<itemlayout height="220" width="327">
					<control type="group">
						<left>21</left>
						<top>21</top>
						<control type="image">
							<top>-5</top>
							<left>-5</left>
							<height>188</height>
							<width>316</width>
							<texture>colors/black.png</texture>
							<bordertexture border="5">shadows/shadow_5px.png</bordertexture>
							<bordersize>5</bordersize>
						</control>
						<control type="gamewindow">
							<width>306</width>
							<height>178</height>
							<scalingmethod>$INFO[ListItem.Property(game.scalingmethod)]</scalingmethod>
							<viewmode>$INFO[ListItem.Property(game.viewmode)]</viewmode>
						</control>
						<control type="image">
							<top>144</top>
							<height>34</height>
							<width>306</width>
							<texture colordiffuse="secondary500">colors/black.png</texture>
						</control>
						<control type="label">
							<top>144</top>
							<height>34</height>
							<width>306</width>
							<align>center</align>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</control>
				</itemlayout>
				<focusedlayout height="220" width="327">
					<control type="group">
						<animation type="Focus">
							<effect type="zoom" start="100" end="103" time="89" center="auto"/>
						</animation>
						<animation type="UnFocus">
							<effect type="zoom" start="103" end="100" time="89" center="auto"/>
						</animation>
						<left>21</left>
						<top>21</top>
						<control type="image">
							<top>-5</top>
							<left>-5</left>
							<animation type="Focus">
								<effect type="fade" start="0" end="100" time="89"/>
							</animation>
							<animation type="UnFocus">
								<effect type="fade" start="100" end="0" time="89"/>
							</animation>
							<width>316</width>
							<height>188</height>
							<texture border="5" colordiffuse="text100">shapes/white_rounded.png</texture>
						</control>
						<control type="image">
							<top>-5</top>
							<left>-5</left>
							<height>188</height>
							<width>316</width>
							<texture>colors/black.png</texture>
							<bordertexture border="5">shadows/shadow_5px.png</bordertexture>
							<bordersize>5</bordersize>
						</control>
						<control type="gamewindow">
							<width>306</width>
							<height>178</height>
							<scalingmethod>$INFO[ListItem.Property(game.scalingmethod)]</scalingmethod>
							<viewmode>$INFO[ListItem.Property(game.viewmode)]</viewmode>
						</control>
						<control type="image">
							<top>144</top>
							<height>34</height>
							<width>306</width>
							<texture colordiffuse="secondary500">colors/black.png</texture>
						</control>
						<control type="label">
							<top>144</top>
							<height>34</height>
							<width>306</width>
							<align>center</align>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</control>
				</focusedlayout>
			</control>
		</control>
	</include>

