<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="DefaultDialogSelect">
		<control type="group">
			<width>1482</width>
			<height>691</height>
			<centertop>50%</centertop>
			<centerleft>50%</centerleft>
			<include content="dialog_panel">
				<param name="width">1482</param>
				<param name="height">691</param>
				<param name="label_id">1</param>
			</include>
			<!-- Background second panel /-->
			<control type="image">
				<top>123</top>
				<left>34</left>
				<texture border="5" colordiffuse="secondary700">shapes/white_rounded.png</texture>
				<height>534</height>
				<width>1003</width>
			</control>
			<!-- List image /-->
			<control type="group">
				<top>311</top>
				<left>1053</left>
				<include content="common_poster">
					<param name="texture_var">$VAR[dialogselect_image_var]</param>
					<param name="fadetime">300</param>
					<param name="width">400</param>
					<param name="height">351</param>
					<param name="bordersize">5</param>
					<param name="bordertexture">shadows/shadow_5px.png</param>
					<param name="aspectratio">keep</param>
				</include>
			</control>
			<!-- File list /-->
			<include content="dialog_list">
				<param name="id">3</param>
				<param name="onright">9001</param>
				<param name="width">1003</param>
				<param name="height">534</param>
				<param name="top">123</param>
				<param name="left">34</param>
				<param name="label_width">961</param>
			</include>
			<include content="dialog_list">
				<param name="id">6</param>
				<param name="onright">9001</param>
				<param name="width">1003</param>
				<param name="height">534</param>
				<param name="top">123</param>
				<param name="left">34</param>
				<param name="label_width">961</param>
			</include>
			<!-- Right aligned buttons /-->
			<control type="grouplist" id="9001">
				<onleft>3</onleft>
				<itemgap>21</itemgap>
				<top>123</top>
				<left>1058</left>
				<!-- Get more /-->
				<include content="dialog_button">
					<param name="id">5</param>
					<param name="width">390</param>
					<param name="label">$LOCALIZE[31062]</param>
					<param name="texturecolor">main900</param>
					<param name="disabledcolor">text900</param>
				</include>
				<!-- Cancel /-->
				<include content="dialog_button">
					<param name="id">7</param>
					<param name="width">390</param>
					<param name="label">$LOCALIZE[31055]</param>
					<param name="texturecolor">main900</param>
					<param name="disabledcolor">text900</param>
				</include>
			</control>
		</control>
	</include>

	<include name="GameDialogSelectView">
		<control type="group">
			<width>1397</width>
			<height>377</height>
			<centertop>50%</centertop>
			<centerleft>50%</centerleft>
			<include content="dialog_panel">
				<param name="width">1397</param>
				<param name="height">377</param>
