<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<window type="dialog">
	<backgroundcolor>0</backgroundcolor>
	<zorder>3</zorder>
	<controls>
		<control type="group">
			<animation type="Conditional" condition="true">
				<effect type="fade" start="0" end="66" time="0"/>
			</animation>
			<include>common_dialog_window_animation</include>
			<!-- Background panel /-->
			<control type="image">
				<height>144</height>
				<width>610</width>
				<texture border="10" colordiffuse="primary100">shapes/white_rounded_10.png</texture>
				<top>55</top>
				<left>1255</left>
			</control>
			<!-- Dialog items /-->
			<control type="grouplist">
				<usecontrolcoords>true</usecontrolcoords>
				<top>76</top>
				<left>1276</left>
				<itemgap>5</itemgap>
				<!-- Header /-->
				<control type="label" id="30">
					<align>center</align>
					<height>24</height>
					<width>568</width>
					<font>opensans_bold_24</font>
					<textcolor>text900</textcolor>
				</control>
				<!-- Text /-->
				<control type="label" id="31">
					<align>center</align>
					<height>26</height>
					<width>568</width>
					<textcolor>text800</textcolor>
					<font>opensans_semibold_24</font>
				</control>
				<control type="progress" id="32">
					<top>19</top>
					<width>568</width>
					<height>21</height>
					<texturebg border="5" colordiffuse="primary700">shapes/white_rounded.png</texturebg>
					<midtexture border="5" colordiffuse="focus">shapes/white_rounded.png</midtexture>
				</control>
			</control>
		</control>
	</controls>
</window>
