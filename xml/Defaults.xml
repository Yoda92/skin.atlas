<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- Containers /-->
	<default type="list">
		<scrolltime tween="sine">400</scrolltime>
		<onup>noop</onup>
		<ondown>noop</ondown>
		<onleft>noop</onleft>
		<onright>noop</onright>
		<preloaditems>2</preloaditems>
		<orientation>vertical</orientation>
	</default>
	<default type="fixedlist">
		<scrolltime tween="sine">400</scrolltime>
		<focusposition>1</focusposition>
		<onup>noop</onup>
		<ondown>noop</ondown>
		<onleft>noop</onleft>
		<onright>noop</onright>
		<preloaditems>2</preloaditems>
		<orientation>horizontal</orientation>
	</default>
	<default type="panel">
		<scrolltime tween="sine">400</scrolltime>
		<onup>noop</onup>
		<ondown>noop</ondown>
		<onleft>noop</onleft>
		<onright>noop</onright>
		<preloaditems>2</preloaditems>
		<orientation>vertical</orientation>
	</default>
	<default type="wraplist">
		<scrolltime tween="sine">400</scrolltime>
		<onup>noop</onup>
		<ondown>noop</ondown>
		<onleft>noop</onleft>
		<onright>noop</onright>
		<orientation>vertical</orientation>
	</default>
	<default type="grouplist">
		<onleft>noop</onleft>
		<onright>noop</onright>
		<onup>noop</onup>
		<ondown>noop</ondown>
		<orientation>vertical</orientation>
		<itemgap>0</itemgap>
	</default>

	<!-- Focusable controls /-->
	<default type="button">
		<pulseonselect>false</pulseonselect>
		<textcolor>text100</textcolor>
		<disabledcolor>text500</disabledcolor>
		<selectedcolor>selected500</selectedcolor>
		<font>sourcesanspro_regular_26</font>
		<aligny>center</aligny>
		<align>center</align>
	</default>
	<default type="togglebutton">
		<pulseonselect>false</pulseonselect>
		<textcolor>text100</textcolor>
		<disabledcolor>text500</disabledcolor>
		<font>sourcesanspro_regular_26</font>
		<aligny>center</aligny>
		<align>center</align>
	</default>
	<default type="radiobutton">
		<pulseonselect>false</pulseonselect>
		<textcolor>text100</textcolor>
		<disabledcolor>text500</disabledcolor>
		<font>sourcesanspro_regular_26</font>
		<aligny>center</aligny>
		<textoffsetx>21</textoffsetx>
		<textureradioonnofocus colordiffuse="text100">buttons/buttons/toggle_right.png</textureradioonnofocus>
		<textureradioonfocus colordiffuse="text100">buttons/toggle_right.png</textureradioonfocus>
		<textureradiooffnofocus colordiffuse="text200">buttons/toggle_left.png</textureradiooffnofocus>
		<textureradioofffocus colordiffuse="text200">buttons/toggle_left.png</textureradioofffocus>
		<textureradioondisabled colordiffuse="text500">buttons/toggle_right.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="text500">buttons/toggle_right.png</textureradiooffdisabled>
	</default>
	<default type="spincontrolex">
		<pulseonselect>false</pulseonselect>
		<aligny>center</aligny>
		<textcolor>text100</textcolor>
		<disabledcolor>text500</disabledcolor>
		<font>sourcesanspro_regular_26</font>
		<textoffsetx>21</textoffsetx>
		<textureup colordiffuse="text300">buttons/toggle_up.png</textureup>
		<textureupfocus colordiffuse="text100">buttons/toggle_up.png</textureupfocus>
		<textureupdisabled colordiffuse="text500">buttons/toggle_up.png</textureupdisabled>
		<texturedown colordiffuse="text300">buttons/toggle_down.png</texturedown>
		<texturedownfocus colordiffuse="text100">buttons/toggle_down.png</texturedownfocus>
		<texturedowndisabled colordiffuse="text500">buttons/toggle_down.png</texturedowndisabled>
		<spinheight>34</spinheight>
		<spinwidth>34</spinwidth>
	</default>
	<default type="sliderex">
		<sliderwidth>144</sliderwidth>
		<sliderheight>13</sliderheight>
		<texturesliderbar border="5" colordiffuse="accent300">shapes/white_rounded.png</texturesliderbar>
		<textureslidernib border="5" colordiffuse="accent100">shapes/white_rounded.png</textureslidernib>
		<textureslidernibfocus border="5">shapes/red500_rounded.png</textureslidernibfocus>
		<texturefocus border="5">shapes/accent500_rounded.png</texturefocus>
		<pulseonselect>false</pulseonselect>
		<textcolor>text100</textcolor>
		<disabledcolor>accent500</disabledcolor>
		<font>sourcesanspro_regular_26</font>
		<aligny>center</aligny>
		<textoffsetx>21</textoffsetx>
	</default>
	<default type="edit">
		<font>sourcesanspro_regular_26</font>
		<textcolor>text100</textcolor>
		<align>center</align>
		<aligny>center</aligny>
		<texturenofocus border="5" colordiffuse="accent800">shapes/white_rounded.png</texturenofocus>
		<texturefocus border="5" colordiffuse="accent500">shapes/white_rounded.png</texturefocus>
	</default>
	<default type="scrollbar">
		<texturesliderbar border="5" colordiffuse="accent300">shapes/white_rounded.png</texturesliderbar>
		<texturesliderbarfocus border="5" colordiffuse="main500">shapes/white_rounded.png</texturesliderbarfocus>
		<showonepage>false</showonepage>
	</default>

	<!-- Other controls /-->
	<default type="label">
		<aligny>center</aligny>
		<label>$INFO[ListItem.Label]</label>
		<font>sourcesanspro_regular_26</font>
		<textcolor>text100</textcolor>
		<disabledcolor>accent500</disabledcolor>
		<selectedcolor>selected500</selectedcolor>
	</default>
	<default type="textbox">
		<label>$INFO[ListItem.Label]</label>
		<font>sourcesanspro_regular_26</font>
		<textcolor>text100</textcolor>
	</default>
	<default type="progress">
		<lefttexture/>
		<righttexture/>
		<overlaytexture/>
	</default>
</includes>
