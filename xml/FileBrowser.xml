<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<window>
	<backgroundcolor>0</backgroundcolor>
	<defaultcontrol always="true">450</defaultcontrol>
	<controls>
		<control type="group">
			<include>common_window_animation</include>
			<include>dialog_background</include>
			<!-- Dialog items /-->
			<control type="group">
				<width>1482</width>
				<height>691</height>
				<centertop>50%</centertop>
				<centerleft>50%</centerleft>
				<include content="dialog_panel">
					<param name="width">1482</param>
					<param name="height">691</param>
					<param name="label_id">411</param>
				</include>
				<!-- Background second panel /-->
				<control type="image">
					<top>123</top>
					<left>34</left>
					<texture border="5" colordiffuse="secondary700">shapes/white_rounded.png</texture>
					<height>534</height>
					<width>1003</width>
				</control>
				<!-- List image /-->
				<control type="group">
					<top>306</top>
					<left>1053</left>
					<include content="common_poster">
						<param name="width">400</param>
						<param name="height">356</param>
						<param name="fadetime">200</param>
						<param name="bordersize">5</param>
						<param name="bordertexture">shadows/shadow_5px.png</param>
						<param name="aspectratio">keep</param>
						<param name="texture_var">$INFO[Container(450).ListItem.Icon]</param>
					</include>
				</control>
				<!-- File list /-->
				<include content="dialog_list">
					<param name="id">450</param>
					<param name="onright">9000</param>
					<param name="width">1003</param>
					<param name="height">534</param>
					<param name="top">123</param>
					<param name="left">34</param>
					<param name="label_width">961</param>
				</include>
				<!-- Right aligned buttons /-->
				<control type="grouplist" id="9000">
					<onleft>450</onleft>
					<itemgap>21</itemgap>
					<top>123</top>
					<left>1058</left>
					<!-- Add /-->
					<include content="dialog_button">
						<param name="id">413</param>
						<param name="width">390</param>
						<param name="label">$LOCALIZE[31052]</param>
						<param name="texturecolor">main900</param>
						<param name="disabledcolor">text900</param>
					</include>
					<!-- Cancel /-->
					<include content="dialog_button">
						<param name="id">414</param>
						<param name="width">390</param>
						<param name="label">$LOCALIZE[31055]</param>
						<param name="texturecolor">main900</param>
						<param name="disabledcolor">text900</param>
					</include>
				</control>
			</control>
		</control>
	</controls>
</window>
